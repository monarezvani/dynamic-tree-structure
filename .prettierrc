{
    "$schema": "http://json.schemastore.org/prettierrc",
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "printWidth": 100,
    "singleQuote": false,
    "bracketSameLine": true,
    "arrowParens": "avoid",
    "importOrder": ["^react$", "^[^.@].*", "^@(.*)$", "^[./]"],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
